@b:"14_COŚ + nieciągłość + PRZYMIOTNIK w gen._2" (
	and(
		in(["coś"], base[0]),
		inter(class[0], {subst,ger,depr}),
		inter(class[2], {adj,ppas,pact}),
		inter(cas[2], {gen}),

		setvar($Pos1, 0),
		setvar($Pos2, 2)
	)
)

@b:"14_PRZYMIOTNIK w gen. + nieciągłość + COŚ_2" (
	and(
		in(["coś"], base[2]),
		inter(class[2], {subst,ger,depr}),
		inter(class[0], {adj,ppas,pact}),
		inter(cas[0], {gen}),

		setvar($Pos1, 2),
		setvar($Pos2, 0)
	)
)
